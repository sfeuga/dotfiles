Include /Users/sfo/.colima/ssh_config

# Default
Host *
    ControlMaster auto
    ControlPersist 4h
    UseKeychain yes

Host cartier
    Hostname github.com
    User git
    ForwardAgent yes
    PreferredAuthentications publickey
    IdentityFile /Users/sfo/.ssh/MacBook-Air_Cartier
    IdentitiesOnly yes

Host gh
    Hostname github.com
    User git
    ForwardAgent yes
    PreferredAuthentications publickey
    IdentityFile /Users/sfo/.ssh/MacBook-Air
    IdentitiesOnly yes

Host feral
    Hostname thalassa.feralhosting.com
    User tsu3d
    IdentityFile /Users/sfo/.ssh/MacBook-Air_Feral

Host jellyWifi
    Hostname 192.168.100.240
    User sfo
    IdentityFile /Users/sfo/.ssh/Macbook-Air

Host jellyEther
    Hostname 192.168.100.241
    User sfo
    IdentityFile /Users/sfo/.ssh/Macbook-Air

