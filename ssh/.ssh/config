Include $HOME/.colima/ssh_config

# Default
Host *
    ControlMaster auto
    ControlPersist 4h
    UseKeychain yes

Host cartier
    Hostname github.com
    User git
    ForwardAgent yes
    PreferredAuthentications publickey
    IdentityFile /Users/sfo/.ssh/MacBook-Air_Cartier
    IdentitiesOnly yes

Host gh
    Hostname github.com
    User git
    ForwardAgent yes
    PreferredAuthentications publickey
    IdentityFile /Users/sfo/.ssh/MacBook-Air
    IdentitiesOnly yes

